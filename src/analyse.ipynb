  {
   "cell_type": "code",
   "execution_count": null,
   "id": "learning-curve",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Courbe d'apprentissage\n",
    "from sklearn.pipeline import Pipeline\n",
    "\n",
    "# Créer un pipeline pour la courbe d'apprentissage\n",
    "pipeline = Pipeline([\n",
    "    ('tfidf', TfidfVectorizer(stop_words='english')),\n",
    "    ('logreg', LogisticRegression(max_iter=1000))\n",
    "])\n",
    "\n",
    "# Calculer les points de la courbe d'apprentissage\n",
    "train_sizes, train_scores, test_scores = learning_curve(\n",
    "    pipeline, X, y, cv=5, scoring='accuracy',\n",
    "    train_sizes=np.linspace(0.1, 1.0, 10), random_state=42)\n",
    "\n",
    "# Calculer les moyennes et écarts-types\n",
    "train_mean = np.mean(train_scores, axis=1)\n",
    "train_std = np.std(train_scores, axis=1)\n",
    "test_mean = np.mean(test_scores, axis=1)\n",
    "test_std = np.std(test_scores, axis=1)\n",
    "\n",
    "# Créer la figure avec Plotly\n",
    "fig = go.Figure()\n",
    "\n",
    "# Ajouter les courbes d'apprentissage\n",
    "fig.add_trace(go.Scatter(\n",
    "    x=train_sizes,\n",
    "    y=train_mean,\n",
    "    mode='lines+markers',\n",
    "    name='Score d\'entraînement',\n",
    "    line=dict(color='#66b3ff', width=2),\n",
    "    marker=dict(size=8)\n",
    "))\n",
    "\n",
    "fig.add_trace(go.Scatter(\n",
    "    x=train_sizes,\n",
    "    y=test_mean,\n",
    "    mode='lines+markers',\n",
    "    name='Score de validation',\n",
    "    line=dict(color='#ff9999', width=2),\n",
    "    marker=dict(size=8)\n",
    "))\n",
    "\n",
    "# Ajouter les zones d'écart-type\n",
    "fig.add_trace(go.Scatter(\n",
    "    x=np.concatenate([train_sizes, train_sizes[::-1]]),\n",
    "    y=np.concatenate([train_mean + train_std, (train_mean - train_std)[::-1]]),\n",
    "    fill='toself',\n",
    "    fillcolor='rgba(102, 179, 255, 0.2)',\n",
    "    line=dict(color='rgba(255, 255, 255, 0)'),\n",
    "    showlegend=False,\n",
    "    name='Train Std'\n",
    "))\n",
    "\n",
    "fig.add_trace(go.Scatter(\n",
    "    x=np.concatenate([train_sizes, train_sizes[::-1]]),\n",
    "    y=np.concatenate([test_mean + test_std, (test_mean - test_std)[::-1]]),\n",
    "    fill='toself',\n",
    "    fillcolor='rgba(255, 153, 153, 0.2)',\n",
    "    line=dict(color='rgba(255, 255, 255, 0)'),\n",
    "    showlegend=False,\n",
    "    name='Test Std'\n",
    "))\n",
    "\n",
    "fig.update_layout(\n",
    "    title='Courbe d\'Apprentissage',\n",
    "    title_font_size=20,\n",
    "    xaxis_title='Taille de l\'ensemble d\'entraînement',\n",
    "    yaxis_title='Accuracy',\n",
    "    xaxis_title_font_size=16,\n",
    "    yaxis_title_font_size=16,\n",
    "    legend_title_font_size=16,\n",
    "    xaxis=dict(tickformat=',d'),\n",
    "    yaxis=dict(range=[0.7, 1.02]),\n",
    "    width=800,\n",
    "    height=500,\n",
    "    legend=dict(x=0.01, y=0.99, bgcolor='rgba(255, 255, 255, 0.5)')\n",
    ")\n",
    "\n",
    "fig.show()"
   ]
