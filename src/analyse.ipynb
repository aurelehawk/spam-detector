   "id": "feature-importance",
   "metadata": {},
   "source": [
    "## 4. Analyse des Caractéristiques Importantes\n",
    "\n",
    "Examinons les mots les plus importants pour la classification des spams."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "important-features",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Extraire les coefficients du modèle et les noms des caractéristiques\n",
    "feature_names = np.array(tfidf.get_feature_names_out())\n",
    "coefs = logreg.coef_[0]\n",
    "\n",
    "# Trier les coefficients par importance\n",
    "top_positive_idx = np.argsort(coefs)[-15:]  # 15 mots les plus associés aux spams\n",
    "top_negative_idx = np.argsort(coefs)[:15]   # 15 mots les plus associés aux hams\n",
    "\n",
    "top_positive_features = feature_names[top_positive_idx]\n",
    "top_positive_coefs = coefs[top_positive_idx]\n",
    "\n",
    "top_negative_features = feature_names[top_negative_idx]\n",
    "top_negative_coefs = coefs[top_negative_idx]\n",
    "\n",
    "# Créer un graphique pour les mots les plus importants\n",
    "fig = make_subplots(rows=1, cols=2, subplot_titles=('Mots associés aux Spams', 'Mots associés aux Hams'))\n",
    "\n",
    "fig.add_trace(\n",
    "    go.Bar(\n",
    "        x=top_positive_coefs,\n",
    "        y=top_positive_features,\n",
    "        orientation='h',\n",
    "        marker_color='#ff9999',\n",
    "        name='Spam'\n",
    "    ),\n",
    "    row=1, col=1\n",
    ")\n",
    "\n",
    "fig.add_trace(\n",
    "    go.Bar(\n",
    "        x=top_negative_coefs,\n",
    "        y=top_negative_features,\n",
    "        orientation='h',\n",
    "        marker_color='#66b3ff',\n",
    "        name='Ham'\n",
    "    ),\n",
    "    row=1, col=2\n",
    ")\n",
    "\n",
    "fig.update_layout(\n",
    "    title='Mots les Plus Importants pour la Classification',\n",
    "    title_font_size=20,\n",
    "    height=500,\n",
    "    width=1000,\n",
    "    showlegend=False\n",
    ")\n",
    "\n",
    "fig.update_xaxes(title_text='Coefficient', row=1, col=1)\n",
    "fig.update_xaxes(title_text='Coefficient', row=1, col=2)\n",
    "\n",
    "fig.show()"
   ]
  },
